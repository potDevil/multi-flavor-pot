<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!--toolsbar颜色和食物名称-->
    <style name="textToolbarTitle">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#d3d3d3</item>
    </style>

    <style name="subscribe_dividing_line">
        <item name="android:background">@color/dividing_line</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.5dip</item>
        <item name="android:layout_marginTop">14.0dip</item>
        <item name="android:layout_marginBottom">14.0dip</item>
    </style>
</resources>
